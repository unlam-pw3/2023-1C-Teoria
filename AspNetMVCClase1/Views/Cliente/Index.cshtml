@using AspNetMVCClase1.Models.Entidades;

@model  List<Cliente>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{

    List<Cliente> misCliente = Model;

}
<h2>Index Cliente</h2>

<table class="table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Edad</th>
            <th>Razon Social</th>
            <th></th>
        </tr>

    </thead>
    <tbody>
        @foreach(Cliente cli in Model)
        {
        <tr>
            <td>@cli.Id</td>
            <td>@cli.Edad</td>
            <td>@cli.RazonSocial</td>
            <td><a asp-action="Mostrar" asp-route-id="@cli.Id">Mostrar</a></td>
        </tr>
        }
    </tbody>
</table>
